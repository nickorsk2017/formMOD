{
  "name": "formmod",
  "version": "1.5.1",
  "description": "Powerful form system for React with hooks",
  "author": "Stepanov Nickolay",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nickorsk2017/formMOD/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nickorsk2017/formMOD.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "homepage": "http://doc.formmod.org/",
  "keywords": [
    "form",
    "react",
    "hooks",
    "form-validation",
    "react-hooks",
    "optional-input",
    "react-form",
    "react-form-system"
  ],
  "scripts": {
    "build": "microbundle-crl --tsconfig tsconfig-prod.json --compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx ./src",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-eslint": "^10.0.3",
    "copy-webpack-plugin": "^10.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-standard": "^5.0.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.2"
  },
  "files": [
    "dist"
  ]
}
